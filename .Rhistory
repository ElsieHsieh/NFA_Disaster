A_Group <- within(A_Group, Education <- factor(Education,
levels=names(sort(table(Education),decreasing=TRUE))))
ggplot(A_Group, aes(x = Education)) +
geom_bar(aes(fill=Education),stat = "count")+
ggtitle("控制組受測者教育程度")+
theme(plot.title = element_text(hjust = 0.5))+
xlab("教育程度") +
ylab("人數") +
scale_fill_manual(values = c("#F67280","#f4a484","#6f94e3"),name = "圖例")+
geom_text(stat='count',aes(label=..count..),vjust=2)
#災害相關
ggplot(A_Group, aes(x = Disaster_related)) +
geom_bar(aes(fill=Disaster_related),stat = "count")+
ggtitle("控制組：是否(曾)從事災害議題相關的工作或學術研究?")+
theme(plot.title = element_text(hjust = 0.5))+
#xlab("") +
ylab("人數") +
scale_fill_manual(values = c("#F67280","#f4a484","#6f94e3"),name = "圖例")+
geom_text(stat='count',aes(label=..count..),vjust=2)+
theme(axis.title.x=element_blank())
#之前使用過防災網站
ggplot(A_Group, aes(x = Used_Before)) +
geom_bar(aes(fill=Used_Before),stat = "count")+
ggtitle("控制組：之前是否有使用過任一個災害資訊網站?")+
theme(plot.title = element_text(hjust = 0.5))+
#xlab("") +
ylab("人數") +
scale_fill_manual(values = c("#F67280","#f4a484","#6f94e3"),name = "圖例")+
geom_text(stat='count',aes(label=..count..),vjust=2)+
theme(axis.title.x=element_blank())
